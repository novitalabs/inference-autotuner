{
  "task_name": "ome-cpu-verification",
  "description": "Test OME orchestration with CPU backend",
  "model": {
    "id_or_path": "llama-3-2-1b-instruct",
    "namespace": "autotuner"
  },
  "base_runtime": "llama-3-2-1b-instruct-rt",
  "parameters": {
    "tp_size": {
      "type": "choice",
      "values": [1]
    },
    "device": {
      "type": "choice",
      "values": ["cpu"]
    }
  },
  "optimization": {
    "strategy": "grid_search",
    "objective": "minimize_latency",
    "max_iterations": 1,
    "timeout_per_iteration": 900
  },
  "benchmark": {
    "task": "text-to-text",
    "traffic_scenarios": [
      "D(10,10)"
    ],
    "num_concurrency": [1],
    "max_time_per_iteration": 5,
    "max_requests_per_iteration": 10,
    "additional_params": {
      "temperature": "0.0"
    }
  }
}
